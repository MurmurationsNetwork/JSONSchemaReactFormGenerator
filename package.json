{
  "name": "@murmurations/jsrfg",
  "type": "module",
  "version": "1.0.6-pre.4",
  "description": "JSON Schema React Form Generator (JSRFG)",
  "author": "murmurations.network",
  "license": "GPLv3",
  "keywords": [
    "JSON Schema",
    "JSON",
    "Schema",
    "Form",
    "Generator",
    "Murmurations"
  ],
  "exports": {
    ".": {
      "import": {
        "default": "./lib/esm/src/index.mjs"
      },
      "require": {
        "default": "./lib/cjs/src/index.cjs"
      }
    }
  },
  "main": "./lib/esm/src/index.mjs",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && vite build",
    "test": "vitest run --config ./vitest.config.js",
    "semantic-release": "semantic-release",
    "prepack": "npm run build",
    "lint": "eslint --ignore-path .gitignore .",
    "lint-fix": "eslint --ignore-path .gitignore . --fix",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx|jsx|json)\"",
    "format": "npm run prettier -- --write",
    "validate": "npm run format && npm run lint  && npm run build",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,js,tsx,jsx,json}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^15.0.2",
    "@types/jsdom": "^21.1.6",
    "@types/react": "^18.2.79",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.3",
    "vite": "^5.3.2",
    "vitest": "^1.5.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
